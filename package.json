{
  "name": "cv-landing-generator",
  "version": "1.0.0",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:frontend": "cd frontend && ng serve",
    "dev:backend": "node server/server.js",
    "build": "cd frontend && npm install && npx ng build",
    "postinstall": "npm run build",
    "start": "node server/server.js",
    "db:test": "node -e \"require('dotenv').config(); const db = require('./server/database/index'); db.testConnection().then(r => console.log(r)).then(() => db.closeDatabase());\""
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@octokit/rest": "^22.0.0",
    "@supabase/supabase-js": "^2.52.1",
    "archiver": "^7.0.1",
    "axios": "^1.11.0",
    "cors": "^2.8.5",
    "dotenv": "^17.0.1",
    "express": "^4.21.2",
    "express-rate-limit": "^8.0.1",
    "express-validator": "^7.2.1",
    "google-auth-library": "^10.1.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "mammoth": "^1.9.1",
    "marked": "^16.1.2",
    "multer": "^2.0.1",
    "ollama": "^0.5.16",
    "pdf-parse": "^1.1.1",
    "pg": "^8.16.3",
    "validator": "^13.15.23"
  },
  "devDependencies": {
    "concurrently": "^8.2.0"
  }
}
